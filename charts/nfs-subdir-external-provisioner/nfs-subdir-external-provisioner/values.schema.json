{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "replicaCount": {
            "type": "integer"
        },
        "strategyType": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "leaderElection": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "nfs": {
            "type": "object",
            "properties": {
                "server": {
                    "type": "string"
                },
                "path": {
                    "type": "string"
                },
                "mountOptions": {
                    "type": "string"
                },
                "reclaimPolicy": {
                    "type": "string"
                },
                "volumeName": {
                    "type": "string"
                }
            }
        },
        "rbac": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
               "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "storageClass": {
            "type": "object",
            "properties": {
                "accessModes": {
                    "type": "string"
                },
                "allowVolumeExpansion": {
                    "type": "boolean"
                },
                "archiveOnDelete": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "defaultClass": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                },
                "reclaimPolicy": {
                    "type": "string"
                },
                "volumeBindingMode": {
                    "type": "string"
                }
            }
        }
    }
}